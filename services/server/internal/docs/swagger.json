{
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "description": "REST API for notebooks, pages, categories, and workspaces.",
        "title": "Notebook API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:3001",
    "basePath": "/",
    "paths": {
        "/notebooks": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "notebooks"
                ],
                "summary": "List all notebooks",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Notebook"
                            }
                        }
                    }
                }
            },
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "notebooks"
                ],
                "summary": "Create a notebook",
                "parameters": [
                    {
                        "description": "Create notebook payload",
                        "name": "notebook",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.CreateNotebookRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/models.Notebook"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/notebooks/{id}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "notebooks"
                ],
                "summary": "Get a notebook by ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Notebook ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Notebook"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/notebooks/{id}/pages": {
            "get": {
                "description": "Alias for GetPagesByNotebook (kept for compatibility)",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "pages"
                ],
                "summary": "Get pages by notebook",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Notebook ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Page"
                            }
                        }
                    }
                }
            },
            "post": {
                "description": "Creates a new page belonging to a notebook",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "pages"
                ],
                "summary": "Create a page in a notebook",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Notebook ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Page payload",
                        "name": "page",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/models.Page"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/pages": {
            "get": {
                "description": "Returns all pages from all notebooks",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "pages"
                ],
                "summary": "Get all pages",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Page"
                            }
                        }
                    }
                }
            }
        },
        "/pages/{id}": {
            "get": {
                "description": "Returns a single page by its ID",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "pages"
                ],
                "summary": "Get a page by ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Page ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Page"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/workspaces": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "workspaces"
                ],
                "summary": "List all workspaces",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Workspace"
                            }
                        }
                    }
                }
            },
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "workspaces"
                ],
                "summary": "Create a workspace",
                "parameters": [
                    {
                        "description": "Workspace payload",
                        "name": "workspace",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/models.Workspace"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.CreateNotebookRequest": {
            "description": "A notebook containing pages.",
            "type": "object",
            "required": [
                "title"
            ],
            "properties": {
                "collaboratorIds": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "description": {
                    "type": "string"
                },
                "isPublic": {
                    "type": "boolean"
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "title": {
                    "type": "string",
                    "minLength": 1
                },
                "workspaceId": {
                    "type": "string"
                }
            }
        },
        "models.Notebook": {
            "description": "A notebook containing pages.",
            "type": "object",
            "properties": {
                "collaboratorIds": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "user-1",
                        "user-2"
                    ]
                },
                "createdAt": {
                    "description": "Timestamps (managed by GORM)",
                    "type": "string"
                },
                "description": {
                    "type": "string",
                    "example": "Study notes"
                },
                "id": {
                    "type": "string",
                    "example": "nbk-123"
                },
                "isPublic": {
                    "type": "boolean",
                    "example": false
                },
                "lastAccessedAt": {
                    "type": "string"
                },
                "ownerId": {
                    "type": "string",
                    "example": "user-123"
                },
                "pages": {
                    "description": "Has Many",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Page"
                    }
                },
                "tags": {
                    "description": "JSON fields (let GORM marshal automatically)",
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "go",
                        "api"
                    ]
                },
                "title": {
                    "type": "string",
                    "example": "My Notebook"
                },
                "updatedAt": {
                    "type": "string"
                },
                "workspaceId": {
                    "description": "Relationships",
                    "type": "string"
                }
            }
        },
        "models.Page": {
            "description": "A content page inside a notebook.",
            "type": "object",
            "properties": {
                "content": {
                    "type": "object"
                },
                "createdAt": {
                    "type": "integer",
                    "example": 1700000000000
                },
                "id": {
                    "type": "string",
                    "example": "page-123"
                },
                "lastExecutedAt": {
                    "type": "integer",
                    "example": 1700002000000
                },
                "notebookId": {
                    "description": "relationships",
                    "type": "string"
                },
                "order": {
                    "type": "integer",
                    "example": 1
                },
                "title": {
                    "type": "string",
                    "example": "Intro"
                },
                "updatedAt": {
                    "type": "integer",
                    "example": 1700001000000
                }
            }
        },
        "models.Workspace": {
            "description": "A workspace that contains notebooks and categories.",
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "workspace-123"
                },
                "notebooks": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Notebook"
                    }
                },
                "userId": {
                    "type": "string",
                    "example": "user-123"
                }
            }
        }
    }
}